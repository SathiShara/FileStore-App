<?xml version="1.0" encoding="utf-8"?> 
<androidx.constraintlayout.widget.ConstraintLayout 
xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:app="http://schemas.android.com/apk/res-auto" 
    xmlns:tools="http://schemas.android.com/tools" 
    android:layout_width="match_parent" 
    android:layout_height="match_parent" 
    android:background="@drawable/bg5" 
    tools:context=".ExcelFilesActivity"> 
 
    <TextView 
        android:id="@+id/textExcelFiles" 
        android:layout_width="wrap_content" 
        android:layout_height="wrap_content" 
        android:drawableLeft="?attr/actionModeCloseDrawable" 
        android:text="Excel Files " 
        android:textColor="@color/material_on_primary_emphasis_medium" 
        android:textSize="24sp" 
        android:textStyle="bold" 
        app:drawableTint="@color/colorWhite" 
        app:layout_constraintBottom_toBottomOf="parent" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.154" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toTopOf="parent" 
        app:layout_constraintVertical_bias="0.037" /> 
 
    <View 
        android:id="@+id/dividerExcelFiles" 
        android:layout_width="409dp" 
        android:layout_height="2dp" 
        android:background="?android:attr/listDivider" 
        android:backgroundTint="@color/white" 
        app:layout_constraintBottom_toBottomOf="parent" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toTopOf="parent" 
        app:layout_constraintVertical_bias="0.093" /> 
 
    <Button 
        android:id="@+id/buttonUploadExcel" 
        android:layout_width="318dp" 
        android:layout_height="52dp" 
        android:layout_marginTop="100dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Select File you want to upload" 
        android:textAllCaps="false" 
        android:textSize="16sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.494" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toTopOf="parent" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder1" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 1" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintTop_toBottomOf="@+id/buttonUploadExcel" 
 
        tools:layout_editor_absoluteX="49dp" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder3" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 3" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.187" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder1" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder4" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 4" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.835" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder2" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder5" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="12dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 5" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.187" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder3" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder6" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="12dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 6" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.835" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder4" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder7" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 7" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.187" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder5" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder8" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 8" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintEnd_toEndOf="parent" 
        app:layout_constraintHorizontal_bias="0.835" 
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder6" /> 
 
    <Button 
        android:id="@+id/buttonExcelFolder2" 
        android:layout_width="150dp" 
        android:layout_height="86dp" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#2DDF7EEF" 
        android:text="Folder 2" 
        android:textAllCaps="false" 
        android:textSize="18sp" 
        android:textStyle="bold" 
        app:layout_constraintTop_toBottomOf="@+id/buttonUploadExcel" 
        tools:layout_editor_absoluteX="218dp" /> 
 
    <ImageButton 
        android:id="@+id/imageButtonExcelAdd" 
        android:layout_width="54dp" 
        android:layout_height="wrap_content" 
        android:layout_marginTop="20dp" 
        android:backgroundTint="#00FFFFFF" 
        android:visibility="visible" 
        app:layout_constraintTop_toBottomOf="@+id/buttonExcelFolder8" 
        app:srcCompat="@drawable/ic_baseline_add_circle_24" 
        tools:layout_editor_absoluteX="314dp" /> 
 
</androidx.constraintlayout.widget.ConstraintLayout> 